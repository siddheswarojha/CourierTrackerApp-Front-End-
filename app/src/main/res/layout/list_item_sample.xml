<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardNews"
    android:layout_width="match_parent"
    android:layout_height="300dp"
    android:layout_margin="8dp"


    android:background="#FF0000"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">




        <TextView
            android:id="@+id/txtOrderId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="240dp"
            android:layout_marginRight="240dp"
            android:text="Order Id"
            android:textColor="#030403" />
        <TextView
            android:id="@+id/txtName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="40dp"
            android:layout_marginEnd="240dp"
            android:layout_marginRight="240dp"
            android:text="name"
            android:textColor="#030403" />
        <TextView
            android:id="@+id/txtItemType"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="70dp"
            android:layout_marginEnd="240dp"
            android:layout_marginRight="240dp"
            android:text="itemType"
            android:textColor="#030403" />

        <TextView
            android:id="@+id/txtStartLocation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="100dp"
            android:text="startLocation"
            android:textColor="#030403" />

        <TextView
            android:id="@+id/txtCurrentLocation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="130dp"
            android:text="currentLocation"
            android:textColor="#030403" />

        <TextView
            android:id="@+id/txtExpectedDeliveryDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="160dp"
            android:text="expectedDeliveryDate"
            android:textColor="#030403" />
        <TextView
            android:id="@+id/txtStatus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="200dp"
            android:layout_marginEnd="240dp"
            android:layout_marginRight="240dp"
            android:text="status"
            android:textColor="#030403" />

        <TextView
            android:id="@+id/txtDeliveryLocation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="69dp"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="240dp"
            android:text="deliveryLocation"
            android:textColor="#030403" />




    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>